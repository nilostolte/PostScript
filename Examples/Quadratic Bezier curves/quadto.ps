50 110 translate
0.2 -0.2 scale


/vsub {
   3 -1 roll exch sub 3 -2 roll sub exch
}def

/vadd {
   3 -1 roll add 3 -2 roll add exch
}def

/vmul {
	/fact exch def
	fact mul exch fact mul exch
} def

/quadto {
	/P2 [ 4 -2 roll ] cvx def
	/P1 [ 4 -2 roll ] cvx def
	/P0 [ currentpoint ] cvx def
	P1 P0 vsub 2 3 div vmul P0 vadd 
	P1 P2 vsub 2 3 div vmul P2 vadd  
	P2 curveto
} def

