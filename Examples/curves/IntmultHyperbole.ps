%!PS-Adobe-2.0 EPSF-2.0
%%BoundingBox: -1 -1 595 841
/plot {
	/function exch def
        /limsup exch def
	/liminf exch def
	/inc exch def
	liminf dup function 1. 0 360 arc fill
      liminf dup function moveto
	liminf inc add inc limsup
		{ dup function lineto  } for
	} def

1 setlinewidth

35 30 translate

-30 0 moveto 550 0 rlineto stroke
0 -20 moveto 0 800 rlineto stroke


520 0 moveto 0 -2 rlineto 7 2 rlineto -7 2 rlineto
0 -2 rlineto fill

0 780 moveto
-2 0 rlineto 2 7 rlineto 2 -7 rlineto
-2 0 rlineto fill


/sc 90 def

newpath
1 11 500 { sc div 1. exch div sc mul } plot
stroke


1 0 0 setrgbcolor
0 2  sc mul moveto   2  sc mul 0 lineto stroke
gsave 0 setgray 1 sc mul dup 2. 0 360 arc fill grestore 
2. 3. div sc mul  4. 3. div sc mul 2. 0 360 arc fill
0 4  sc mul moveto   1  sc mul 0 lineto stroke
gsave 0 setgray 0.5 sc mul 2 sc mul 2. 0 360 arc fill grestore 
1. 3. div sc mul 8. 3. div sc mul 2. 0 360 arc fill
0 8 sc mul moveto   0.5  sc mul 0 lineto stroke
gsave 0 setgray 0.25 sc mul 4 sc mul 2. 0 360 arc fill grestore 
0 16 sc mul moveto  0.25  sc mul 0 lineto stroke
gsave 0 setgray 0.125 sc mul 8 sc mul 2. 0 360 arc fill grestore
1. 6. div sc mul  16. 3. div sc mul 2. 0 360 arc fill 

0.5 sc mul 2 sc mul moveto 0 -50 rlineto stroke

showpage

